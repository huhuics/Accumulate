--1.创建序列
CREATE SEQUENCE SEQ_NAME  --序列名
INCREMENT BY 1    
START WITH 1  --从1开始  
NOMAXVALUE    --不设最大值  
NOCYCLE       --不循环  
NOCACHE;  

--2.创建触发器
create or replace trigger TRI_NAME  --触发器名
before insert on TABLE_NAME  --表名
for each row
begin
select SEQ_NAME.nextval into :new.ID from dual;  --sequence名
end;